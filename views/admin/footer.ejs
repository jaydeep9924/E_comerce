<script>

function getSubcategoryData()
   {
    var catId = document.getElementById('categoryId').value;
    $.ajax({
        url : '/extracategory/getSubcategoryData',
        method : 'post',
        data :{
            'categoryId' : catId
        },
        success : (res)=>{
            console.log(res);
            $('#subcategoryId').html(res);
        }
    })
   }


   function getextracategoryData(){

    var extraId = document.getElementById('subcategoryId').value;
    $.ajax({
        url : '/brand/getextracategorydata',
        method : 'post',
        data : {
            'subcategoryId' : extraId
        },
        success :(res)=>{
            console.log(res);
            $('#extracategoryId').html(res)
        }
    })
   }


   function brandtypedata(){

    var catId = document.getElementById('categoryId').value;
    var subId = document.getElementById('subcategoryId').value;
    let extraId = document.getElementById('extracategoryId').value;
    $.ajax({
        method : 'post',
        url : '/product/getbrandtypedata',
        data :{
            catrecord : catId,
            subrecord : subId,
            extrecord : extraId,
        },
        success : (res)=>{
           $('#getbrandtype').html(res);
        }
    })
   }

</script>

<script>

    <% if(flash.success && flash.success.length > 0) { %>
        new Noty({
          type: 'success',
          text: '<%= flash.success %>',
          layout : 'topCenter',
          timeout : 1000,
          theme: 'metroui',
            
        }).show();
    <% } %>
    
    
    <% if(flash.error && flash.error.length > 0) { %>
        new Noty({
          type: 'error',
